{
  "name": "f1",
  "version": "1.0.0",
  "description": "f1 exercise app",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "pull-mssql-docker": "docker pull mcr.microsoft.com/mssql/server:2019-latest",
    "database-mssql": "docker run --net=f1-net --name mssql -e \"ACCEPT_EULA=Y\" -e \"MSSQL_SA_PASSWORD=notPass123\" -p 1433:1433 -v C:/data/MSSQL/data:/var/opt/mssql/data -v C:/data/MSSQL/log:/var/opt/mssql/log -v C:/data/MSSQL/secrets:/var/opt/mssql/secrets -d mcr.microsoft.com/mssql/server:2019-latest", 
    "be-build-docker": "cd be & docker build -t 91kovacsk/f1-be .",
    "be-run-docker": "docker run -p 4000:4000 --net=f1-net --name f1-be-cont 91kovacsk/f1-be",
    "be": "cd be & npm start",
    "fe-build-docker": "cd fe & docker build -t 91kovacsk/f1-fe .",
    "fe-run-docker": "docker run -p 3000:3000 --net=f1-net --name f1-fe-cont 91kovacsk/f1-fe",
    "fe": "cd fe & npm start",
    "clear-docker-containers": "docker rm -f f1-be-cont f1-fe-cont",
    "clear-docker-images": "docker rmi 91kovacsk/f1-be 91kovacsk/f1-fe",
    "create-docker-network": "docker network create --driver bridge f1-net"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/91-kovacs-k/f1.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/91-kovacs-k/f1/issues"
  },
  "homepage": "https://github.com/91-kovacs-k/f1#readme"
}
